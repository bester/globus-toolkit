service gsiftp
{
instances               = 100
socket_type             = stream
wait                    = no
user                    = root
env                     += GLOBUS_LOCATION=@prefix@
env                     += LD_LIBRARY_PATH=@conf_libdir@
server                  = @conf_sbindir@/globus-gridftp-server
server_args             = -i
log_on_success          += DURATION
nice                    = 10
disable                 = yes
}
